if exist ("isfolder") == 0
 if (isdir (fullfile (fileparts (mfilename ("fullpath")), "sensors")))
   addpath (fullfile (fileparts (mfilename ("fullpath")), "sensors"));
 endif
else
 if (isfolder (fullfile (fileparts (mfilename ("fullpath")), "sensors")))
   addpath (fullfile (fileparts (mfilename ("fullpath")), "sensors"));
 endif
endif
